{
    "name": "metises/pusher-notifier",
    "type": "symfony-notifier-bridge",
    "description": "Symfony Pusher Notifier Bridge",
    "keywords": ["pusher", "notifier"],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Yasmany Cubela Medina",
            "email": "yasmanycm@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "pusher/pusher-php-server": "^7.0",
        "symfony/deprecation-contracts": "^2.1|^3",
        "symfony/http-client": "^4.3|^5.0|^6.0",
        "symfony/notifier": "^5.3|^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/event-dispatcher": "^4.3|^5.0|^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": { "Symfony\\Component\\Notifier\\Bridge\\Pusher\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
