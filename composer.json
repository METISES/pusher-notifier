{
  "name": "metises/pusher-notifier",
  "type": "symfony-notifier-bridge",
  "description": "Symfony Pusher Notifier Bridge",
  "keywords": [
    "pusher",
    "notifier"
  ],
  "homepage": "https://symfony.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Yasmany Cubela Medina",
      "email": "yasmanycm@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "pusher/pusher-php-server": "^7.0",
    "symfony/deprecation-contracts": "^2.1|^3",
    "symfony/http-client": "^4.3|^5.0|^6.0",
    "symfony/notifier": "^5.3|^6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/event-dispatcher": "^4.3|^5.0|^6.0",
    "phpunit/phpunit": "^9.5",
    "composer/package-versions-deprecated": "^1.11",
    "friendsofphp/php-cs-fixer": "v3.4",
    "guzzlehttp/guzzle": "^7.4",
    "insolita/unused-scanner": "^2.4",
    "nunomaduro/phpinsights": "^v2.1",
    "phan/phan": "^5.3",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpdoc-parser": "^1.2",
    "phpstan/phpstan": "^1.4",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-php-parser": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "rector/rector": "^0.12",
    "symfony/thanks": "^v1.2",
    "vimeo/psalm": "^4.18",
    "weirdan/doctrine-psalm-plugin": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Symfony\\Component\\Notifier\\Bridge\\Pusher\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "scripts": {
    "rector": "./vendor/bin/rector process",
    "phpstan": "./vendor/bin/phpstan analyse --memory-limit=1024M -c phpstan.neon .",
    "phan": "./vendor/bin/phan",
    "psalm": "./vendor/bin/psalm",
    "csf": "./vendor/bin/php-cs-fixer fix",
    "us": "./vendor/bin/unused_scanner",
    "phpunit": "./vendor/bin/phpunit",
    "amscanner": "amscanner --no-report --path-whitelist=\"/var/www/scanner-whitelist.json\"",
    "phpinsights": "./vendor/bin/phpinsights analyse -n --fix --ansi --format=github-action .",
    "tools": [
      "@rector",
      "@phpstan",
      "@phan",
      "@psalm",
      "@phpinsights",
      "@csf",
      "@us",
      "@amscanner"
    ],
    "test": [
      "@tools",
      "@phpunit"
    ],
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "symfony/thanks": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  }
}
